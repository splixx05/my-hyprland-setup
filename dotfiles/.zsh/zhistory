#!/bin/sh\
\
iface="wlan0"   # oder eth0 / enp3s0 / wlp2s0 etc.\
ssid=$(nmcli -t -f active,ssid dev wifi | grep '^yes' | cut -d: -f2)\
\
echo "ÉøÉ≥ÉÅ Verbindung: $ssid ($iface)"\
echo "==================================="\
\
ifstat -i "$iface" 1\

ifstat -i wlan0 1\

ifstat --version\

install ifstat\

iface=wlan0\
RX1=$(cat /proc/net/dev | grep $iface | awk '{print $2}')\
TX1=$(cat /proc/net/dev | grep $iface | awk '{print $10}')\
sleep 1\
RX2=$(cat /proc/net/dev | grep $iface | awk '{print $2}')\
TX2=$(cat /proc/net/dev | grep $iface | awk '{print $10}')\
RX=$(( (RX2 - RX1) / 1024 ))\
TX=$(( (TX2 - TX1) / 1024 ))\
echo "ÉøÉ≥ÉÅ $iface ‚É¶É≥ ${RX} KB/s ‚É¶É± ${TX} KB/s"\

nu
